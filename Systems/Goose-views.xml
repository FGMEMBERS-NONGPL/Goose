<?xml version="1.0"?>

<!-- Goose Views -->

<PropertyList>

  <view n="0"> <!-- pilot view -->
    <name>Cockpit</name>
    <type>lookfrom</type>
    <internal type="bool">true</internal>
    <config>
      <!-- x/y/z = right/up/back ( Blender/FG equivalent: y/z/x) -->
      <x-offset-m archive="y" type="double">-0.383</x-offset-m>
      <y-offset-m archive="y" type="double">0.900</y-offset-m><!-- 930 -->
      <z-offset-m archive="y" type="double">3.400</z-offset-m>
      <pitch-offset-deg>-6.3</pitch-offset-deg><!-- 9 -->
      <default-field-of-view-deg type="double">66.0</default-field-of-view-deg>
      <limits>
        <enabled type="bool">false</enabled>
      </limits>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y">4.39</target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <default-field-of-view-deg type="double">47.0</default-field-of-view-deg>
      <target-z-offset-m archive="y">4.39</target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
     <target-z-offset-m archive="y">4.39</target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
