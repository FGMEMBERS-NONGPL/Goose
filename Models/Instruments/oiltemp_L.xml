<?xml version="1.0"?>

<PropertyList>

<path>oiltemp.ac</path>

<animation>
  <type>material</type>
  <object-name>Oiltemp.FaceBack</object-name>
  <object-name>Oiltemp.Needle</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Oiltemp.Needle</object-name>
  <property>/engines/engine[0]/oiltempc</property>
  <interpolation>
    <entry><ind>  0</ind><dep>-60</dep></entry>
    <entry><ind>100</ind><dep> 60</dep></entry>
  </interpolation>
  <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>-0.016</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>
